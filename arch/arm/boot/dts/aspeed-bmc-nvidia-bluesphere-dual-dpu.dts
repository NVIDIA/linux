	/*L0-L7*/       "","","","","","","","",
	/*M0-M7*/       "","","","","","","","",
	/*N0-N7*/       "bf2_soc_hard_rst","bf2_nsrst_arm","bf2_arm_soft_reset","","","tor_eswitch_reset","nic_bmc_ctrl0","",
