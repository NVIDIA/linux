&i2c1 {
	status = "okay";
	multi-master;
	// BF to BMC
	ipmb-1@10 {
		compatible = "ipmb-dev";
		// TODO: replace with 0x40000000 with I2C_OWN_SLAVE_ADDRESS
		reg = <(0x10 | 0x40000000)>;
		i2c-protocol;
	};
	// BMC to BF
	ipmb-1@20 {
		compatible = "ipmb-dev";
		// TODO: replace with 0x40000000 with I2C_OWN_SLAVE_ADDRESS
		reg = <(0x20 | 0x40000000)>;
		i2c-protocol;
	};
};

&ehci0 {
	status = "okay";
};