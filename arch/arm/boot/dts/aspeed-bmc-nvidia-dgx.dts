&i2c10 {
	status = "okay";
};

&i2c11 {
	status = "okay";

    // GPU Baseboard FRU
    eeprom@53 {
        compatible = "atmel,24c512";
        reg = <0x53>;
        pagesize = <128>;
        size = <65536>;
    };
