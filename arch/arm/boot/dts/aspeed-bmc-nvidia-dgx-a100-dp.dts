	/*G0-G7*/       "","","","","","","","",
	/*H0-H7*/       "","","","","","","","",
	/*I0-I7*/       "","","bios_spi_cs_sel","bmc_ready","","","","",
	/*J0-J7*/       "","","","","","","","",
	/*K0-K7*/       "","","","","","","","",
	/*L0-L7*/       "","","","","","","","",
